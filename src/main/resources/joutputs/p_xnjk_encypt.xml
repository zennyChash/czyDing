	<joutput id="q_xnjk_decrypt_aes" name="虚拟金库-AES解密" description=""> 
		<jTemplate>
		<![CDATA[
			{
			#if($xnjk_encrypt_aes&&$xnjk_encrypt_aes.rows.size()>0)
				"options":[
					#foreach($row in $xnjk_encrypt_aes.rows)
	   						{"sszb_i":"$!{row.sszb_i}","sszb":"$!{row.sszb}"}#if($velocityCount != $xnjk_encrypt_aes.rows.size()),#end
	   					#end
				]
			#end	
			}
		]]>
		</jTemplate>
		<vDs name="xnjk_encrypt_aes" fields="ny,czfpbm,czfp,ttax,sszb_i,sszb">
            <orderBy name = "ny" dir="desc"/>
		</vDs>
	</joutput>
