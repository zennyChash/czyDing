<?xml version="1.0" encoding="UTF-8"?>
<pages>
	<joutput id="qcqy_df_ny" name="最大日期" description=""> 
		<jTemplate>
		<![CDATA[
			{
				"latestValidMonth":"201806",
				"format":"Ym" 
			}
		]]>
		</jTemplate>
	</joutput>
	<joutput id="qcqy_year" name="有数据的年" description=""> 
		<jTemplate>
		<![CDATA[
			{
				"options":[{"bm":"2018"},{"bm":"2017"}],
				"defaultOp":"2018"
			}
		]]>
		</jTemplate>
	</joutput>
	<joutput id="qcqy_month" name="有数据的月份选项" description=""> 
		<jTemplate>
		<![CDATA[
			{
				"options":[{"bm":"06","name":"06"},{"bm":"05"},{"bm":"04"},{"bm":"03"}],
				"defaultOp":"06"
			}
		]]>
		</jTemplate>
	</joutput>
	<joutput id="qcqy_details" name="迁出企业列表"  description=""> 
		<jTemplate>
		<![CDATA[
			{
				#if($qcEns&&$qcEns.rows.size()>0)
				"qcqy":[
					#foreach($row in $qcEns.rows)
	   					{"swdjzh":"$!{row.swdjzh}","mc":"$!{row.mc}","czfp":"$!{row.czfp}","bnlj":"$!{row.bnlj_o}",
	   					"sntq":"$!{row.sntq_o}","dest":"$!{row.dest}","escdate":"$!{row.escdate}"}#if($velocityCount != $qcEns.rows.size()),#end
	   				#end
				],
				"total": $!{qcEns.total}
				#else
				"qcqy":[],"total":0
				#end
			}
		]]>
		</jTemplate>
		<vDs name="qcEns" refDtSrc="qcqy_details" paging="true" fields="swdjzh,mc,czfp,bnlj_o,sntq_o,dest,escdate">
			<filter name = "ny" value="201806"/>
			<orderBy name = "escdate" dir="desc"/>
		</vDs>
	</joutput>
</pages>

