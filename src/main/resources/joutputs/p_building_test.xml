<?xml version="1.0" encoding="UTF-8"?>
<pages>
	<joutput id="building_df_ny" name="单户查询-最大月份" description=""> 
		<jTemplate>
		<![CDATA[
			{
			#if($bd_maxny&&$bd_maxny.rows.size()>0)
				"latestValidMonth":"$!{bd_maxny.rows[0].ny}",
				"format":"Ym" 
			#end	
			}
		]]>
		</jTemplate>
		<vDs name="bd_maxny" refDtSrc="bd_maxny" fields="ny">
			<filter name = "bid" value="1"/>
		</vDs>
	</joutput>
	<joutput id="building_year" name="单户查询-有数据的年" description=""> 
		<jTemplate>
		<![CDATA[
			{
				
				#if($bd_year&&$bd_year.rows.size()>0)
				"options":[
					#foreach($row in $bd_year.rows)
						#set($Double = 0.0)
						#set( $t = $Double.parseDouble($!{row.nian}) + 666.89 ) 
	   					{"ddd":"$t","bm":"$!{row.nian}","name":"$!{row.nian}"}#if($velocityCount != $bd_year.rows.size()),#end
	   				#end
				],
				"defaultOp":"$!{bd_year.rows[0].nian}"
				#end
			}
		]]>
		</jTemplate>
		<vDs name="bd_year" fields="nian">
			<filter name = "bid" value="1"/>
			<orderBy name = "nian" dir="desc"/>
		</vDs>
	</joutput>
	<joutput id="building_month" name="有数据的月份选项" description=""> 
		<jTemplate>
		<![CDATA[
			{
			#if($bd_month&&$bd_month.rows.size()>0)
				"options":[
					#foreach($row in $bd_month.rows)
	   						{"bm":"$!{row.yue}","name":"$!{row.yue}"}#if($velocityCount != $bd_month.rows.size()),#end
	   					#end
				],
				"defaultOp":"$!{bd_month.rows[0].yue}"
			#end
			}
		]]>
		</jTemplate>
		<vDs name="bd_month" fields="nian,yue">
			<filter name = "bid" value="1"/>
			<filter name = "nian" refParam="year" value="2018"/>
			<orderBy name = "yue" dir="desc"/>
		</vDs>
	</joutput>
	<joutput id="building_all" name="楼宇信息"  description=""> 
		<jTemplate>
		<![CDATA[
			{
				#if($bd_all&&$bd_all.rows.size()>0)
					"buildings":[
						#foreach($row in $bd_all.rows)
		   				{"bid":"$!{row.bid}","mc":"$!{row.mc}"}#if($velocityCount != $bd_all.rows.size()),#end
		   				#end
					],
					"total": $!{bd_all.total}
				#else
					"buildings":[],
					"total":0
				#end
			}
		]]>
		</jTemplate>
		<vDs name="bd_all" refDtSrc="buildingbasic" fields="bid,mc,pyorder" >
			<orderBy name = "pyorder" dataType="integer" dir="asc"/>
		</vDs>
	</joutput>
	<joutput id="building_one" name="单个楼宇信息"  description=""> 
		<jTemplate>
		<![CDATA[
			{
			#if($bd_one&&$bd_one.rows.size()>0)
				"bldmc":"$!{bd_one.rows[0].mc}",
        		"blddz":"$!{bd_one.rows[0].dz}",
				"encount":"$!{bd_one.rows[0].ecount}", 
				"bnlj":"$!{bd_one.rows[0].bnlj}", 
				"sntq":"$!{bd_one.rows[0].sntq}"
			#end
			}
		]]>
		</jTemplate>
		<vDs name="bd_one" fields="bid,mc,dz,ecount,srzb_t,srzb_b,bnlj,sntq,tbzz">
			<filter name = "bid" value="1"/>
			<filter name = "ny" value="201803"/>
		</vDs>
	</joutput>
	<joutput id="building_one_details" name="楼宇入驻企业信息"  description=""> 
		<jTemplate>
		<![CDATA[
			{
			#if($bd_ens&&$bd_ens.rows.size()>0)
				"ens_hy":[
				#foreach($row in $bd_ens.rows)
		   		{"swdjzh":"$!{row.swdjzh}","mc":"$!{row.mc}","bnlj":"$!{row.bnlj}","sntq":"$!{row.sntq}","lyzb":"$!{row.lyzb}"}
		   			#if($velocityCount != $bd_ens.rows.size()),#end
		   		#end
				],
				"total": "$!{bd_ens.total}"
			#end
			}
		]]>
		</jTemplate>
		<vDs name="bd_ens" fields="swdjzh,mc,bnlj,sntq,lyzb" paging="true">
			<filter name = "bid" value="1"/>
			<filter name = "ny" value="201803"/>
			<orderBy name = "pm" dataType="integer" dir="asc"/>
		</vDs>
	</joutput>
	<joutput id="building_one_en" name="入驻企业单户信息"  description=""> 
		<jTemplate>
		<![CDATA[
			{
			#if($bd_en&&$bd_en.rows.size()>0)
				"swdjzh":"$!{bd_en.rows[0].swdjzh}",
				"mc":"$!{bd_en.rows[0].mc}",
				"czfp":"$!{bd_en.rows[0].czfp}",
				"bnlj":"$!{bd_en.rows[0].bnlj}",
				"sntq":"$!{bd_en.rows[0].sntq}"
			#end
			}
		]]>
		</jTemplate>
		<vDs name="bd_en" refDtSrc="bd_ens" fields="swdjzh,mc,czfp,bnlj,sntq,lyzb" paging="true">
			<filter name = "bid" value="1"/>
			<filter name = "ny" value="201803"/>
			<filter name = "swdjzh" value="913300000740226677"/>
		</vDs>
	</joutput>
</pages>

