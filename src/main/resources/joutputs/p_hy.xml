<?xml version="1.0" encoding="UTF-8"?>
<pages>
	<joutput id="hy_df_ym" name="行业分布-最大月份" description=""> 
		<jTemplate>
		<![CDATA[
			{
			#if($hy_maxny&&$hy_maxny.rows.size()>0)
				"latestValidMonth":"$!{hy_maxny.rows[0].ny}",
				"format":"Ym" 
			#end	
			}
		]]>
		</jTemplate>
		<vDs name="hy_maxny" refDtSrc="hy_maxny" fields="ny">
            <orderBy name = "ny" dir="desc"/>
		</vDs>
	</joutput>
	<joutput id="hy_year" name="行业分布-有数据的年" description=""> 
		<jTemplate>
		<![CDATA[
			{
			#if($hy_year&&$hy_year.rows.size()>0)
				"options":[
					#foreach($row in $hy_year.rows)
	   						{"bm":"$!{row.nian}"}#if($velocityCount != $hy_year.rows.size()),#end
	   					#end
				],
				"defaultOp":"$!{hy_year.rows[0].nian}"
			#end	
			}
		]]>
		</jTemplate>
		<vDs name="hy_year" refDtSrc="hy_year" fields="nian">
			<orderBy name = "nian" dir="desc"/>
		</vDs>
	</joutput>
	<joutput id="hy_month" name="行业分布-有数据的月份" description=""> 
		<jTemplate>
		<![CDATA[
			{
			#if($hy_month&&$hy_month.rows.size()>0)
				"options":[
					#foreach($row in $hy_month.rows)
	   						{"bm":"$!{row.yue}","name":"$!{row.yue}"}#if($velocityCount != $hy_month.rows.size()),#end
	   					#end
				],
				"defaultOp":"$!{hy_month.rows[0].yue}"
			#end
			}
		]]>
		</jTemplate>
		<vDs name="hy_month" fields="nian,yue">
			<filter name = "nian" refParam="year" value="2018"/>
			<orderBy name = "yue" dir="desc"/>
		</vDs>
	</joutput>
	<joutput id="hy" name="行业分布"  description=""> 
		<jTemplate>
		<![CDATA[
			{
				"hyInfo":[
				#if($hy_tax_fb&&$hy_tax_fb.rows.size()>0)
					#foreach($row in $hy_tax_fb.rows)
	   					{"hybm":"$!{row.hybm}","hy":"$!{row.hymc}","bnlj":"$!{row.bnlj}","sntq":"$!{row.sntq}",
	   					"srtb":"$!{row.tbzz}","sszb":"$!{row.srzb}","class":"hy_$!{row.hybm}","color":"$!{row.hycolor}"}
	   					#if($velocityCount != $hy_tax_fb.rows.size()),#end
	   				#end
				#end
				]
			}
		]]>
		</jTemplate>
		<vDs name="hy_tax_fb" refDtSrc="hy_tax_fb" fields="hybm,hymc,bnlj,sntq,srzb,tbzz,hycolor">
			<filter name = "ny" value="201806"/>
			<orderBy name = "hybm" dir="asc"/>
		</vDs>
	</joutput>
	<joutput id="hy_rclist" name="某行业的税收前30"  description=""> 
		<jTemplate>
		<![CDATA[
			{
				#if($hyEns&&$hyEns.rows.size()>0)
				"ens":[
					#foreach($row in $hyEns.rows)
	   					{"swdjzh":"$!{row.swdjzh}","mc":"$!{row.mc}"}
	   					#if($velocityCount != $hyEns.rows.size()),#end
	   				#end
				],
				"total": $!{hyEns.total}
				#else
				"ens_hy":[],"total":0
				#end
			}
		]]>
		</jTemplate>
		<vDs name="hyEns" refDtSrc="hy_one_details" paging="true" fields="swdjzh,mc,bnlj,sntq,zszb">
			<filter name = "hybm" value="K"/>
			<filter name = "ny" value="201806"/>
			<orderBy name = "pm" dataType="double" dir="asc"/>
		</vDs>
	</joutput>
	<joutput id="hy_entax" name="某企业的税收情况"  description="某行业中排名前30某企业的税收情况"> 
		<jTemplate>
		<![CDATA[
			{
			#if($hy_one_en&&$hy_one_en.rows.size()>0)
				"swdjzh":"$!{hy_one_en.rows[0].swdjzh}",
				"mc":"$!{hy_one_en.rows[0].mc}",
				"czfp":"$!{hy_one_en.rows[0].czfp}",
				"bnlj":"$!{hy_one_en.rows[0].bnlj}",
				"sntq":"$!{hy_one_en.rows[0].sntq}"
			#else
				"swdjzh":"",
				"mc":"",
				"czfp":"",
				"bnlj":"",  
				"sntq":""
			#end
			}
		]]>
		</jTemplate>
		<vDs name="hy_one_en" refDtSrc="hy_one_details" fields="swdjzh,mc,bnlj,sntq,czfp">
			<filter name = "ny" value="201806"/>
			<filter name = "swdjzh" value="913301006652423388"/>
		</vDs>
	</joutput>
</pages>

