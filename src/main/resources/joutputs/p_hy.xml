<?xml version="1.0" encoding="UTF-8"?>
<pages>
	<joutput id="hy_df_ny" name="最大日期" description=""> 
		<jTemplate>
		<![CDATA[
			{
			#if($hy_maxny&&$hy_maxny.rows.size()>0)
				"latestValidMonth":"$!{hy_maxny.rows[0].ny}",
				"format":"Ym" 
			#end
			}
		]]>
		</jTemplate>
		<vDs name="hy_maxny" refDtSrc="hy_maxny" fields="ny"/>
	</joutput>
	<joutput id="hy_year" name="有数据的年" description=""> 
		<jTemplate>
		<![CDATA[
			{
			#if($hy_year&&$hy_year.rows.size()>0)
				"options":[
					#foreach($row in $hy_year.rows)
	   						{"bm":"$!{row.nian}"}#if($velocityCount != $hy_year.rows.size()),#end
	   					#end
				],
				"defaultOp":"$!{hy_year.rows[0].nian}"
			#end	
			}
		]]>
		</jTemplate>
		<vDs name="hy_year" fields="nian">
			<orderBy name = "nian" dir="desc"/>
		</vDs>
	</joutput>
	<joutput id="hy_month" name="有数据的月份选项" description=""> 
		<jTemplate>
		<![CDATA[
			{
			#if($hy_month&&$hy_month.rows.size()>0)
				"options":[
					#foreach($row in $hy_month.rows)
	   					{"bm":"$!{row.yue}"}#if($velocityCount != $hy_month.rows.size()),#end
	   				#end
				],
				"defaultOp":"$!{hy_month.rows[0].yue}"
			#end
			}
		]]>
		</jTemplate>
		<vDs name="hy_month" fields="ny,yue">
			<filter name = "nian" refParam="year" value="2018"/>
			<orderBy name = "ny" dir="desc"/>
		</vDs>
	</joutput>
	<joutput id="hy_all" name="行业分布"  description=""> 
		<jTemplate>
		<![CDATA[
			{
				"chart":[ 
				#if($hy_tax_fb&&$hy_tax_fb.rows.size()>0)
					#foreach($row in $hy_tax_fb.rows)
	   					{"name":"$!{row.hy}","value":"$!{row.bnlj}"}
	   					#if($velocityCount != $hy_tax_fb.rows.size()),#end
	   				#end
				#end
				],
				"hyTax":[
				#if($hy_tax_fb&&$hy_tax_fb.rows.size()>0)
					#foreach($row in $hy_tax_fb.rows)
	   					{"hybm":"$!{row.hybm}","hy":"$!{row.hy}","bnlj":"$!{row.bnlj}","sntq":"$!{row.sbtq}",
	   					"srzb":"$!{row.srzb}","class":"hy_$!{row.hybm}","color":"$!{row.hycolor}"}
	   					#if($velocityCount != $hy_tax_fb.rows.size()),#end
	   				#end
				#end
				]
			}
		]]>
		</jTemplate>
		<vDs name="hy_tax_fb" refDtSrc="hy_tax_fb" fields="hybm,hy,bnlj,sntq,srzb,hycolor">
			<filter name = "ny" value="201806"/>
			<orderBy name = "hybm" dir="asc"/>
		</vDs>
	</joutput>
	<joutput id="hy_one" name="某行业的税收情况"  description=""> 
		<jTemplate>
		<![CDATA[
			{
			#if($hy_one&&$hy_one.rows.size()>0)
				"bnlj":"$!{hy_one.rows[0].bnlj}",
				"sntq":"$!{hy_one.rows[0].sntq}",
				"tbzz":"$!{hy_one.rows[0].tbzz}"
			#else
				"bnlj":"",  
				"sntq":"",  
				"tbzz":"" 
			#end
			}
		]]>
		</jTemplate>
		<vDs name="hy_one" refDtSrc="hy_tax_fb" fields="bnlj,sntq,tbzz">
			<filter name = "hybm" value="K"/>
			<filter name = "ny" value="201806"/>
		</vDs>
	</joutput>
	<joutput id="hy_one_details" name="某行业的税收前30"  description=""> 
		<jTemplate>
		<![CDATA[
			{
				#if($hyEns&&$hyEns.rows.size()>0)
				"ens_hy":[
					#foreach($row in $hyEns.rows)
	   					{"swdjzh":"$!{row.swdjzh}","mc":"$!{row.mc}","bnlj":"$!{row.bnlj}","sntq":"$!{row.sbtq}","hyzb":"$!{row.hyzb}"}
	   					#if($velocityCount != $hyEns.rows.size()),#end
	   				#end
				],
				"total": $!{hyEns.total}
				#else
				"ens_hy":[],"total":0
				#end
			}
		]]>
		</jTemplate>
		<vDs name="hyEns" refDtSrc="hy_one_details" paging="true" fields="swdjzh,mc,bnlj,sntq,hyzb">
			<filter name = "hybm" value="K"/>
			<filter name = "ny" value="201806"/>
			<orderBy name = "bnlj" dataType="double" dir="desc"/>
		</vDs>
	</joutput>
	<joutput id="hy_one_en" name="某企业的税收情况"  description="某行业中排名前30某企业的税收情况"> 
		<jTemplate>
		<![CDATA[
			{
			#if($hy_one_en&&$hy_one_en.rows.size()>0)
				"swdjzh":"$!{hy_one_en.rows[0].swdjzh}",
				"mc":"$!{hy_one_en.rows[0].mc}",
				"bnlj":"$!{hy_one_en.rows[0].bnlj}",
				"sntq":"$!{hy_one_en.rows[0].sntq}",
				"hyzb":"$!{hy_one_en.rows[0].tbzz}"
			#else
				"swdjzh":"",
				"mc":"",
				"bnlj":"",  
				"sntq":"",  
				"hyzb":"" 
			#end
			}
		]]>
		</jTemplate>
		<vDs name="hy_one_en" refDtSrc="hy_one_details" fields="swdjzh,mc,bnlj,sntq,hyzb">
			<filter name = "ny" value="201806"/>
			<filter name = "swdjzh" value="91330604146165334B"/>
		</vDs>
	</joutput>
</pages>

