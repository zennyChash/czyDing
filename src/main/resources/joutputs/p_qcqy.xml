<?xml version="1.0" encoding="UTF-8"?>
<pages>
	<joutput id="qcqy_df_ym" name="迁出企业-最大月份" description=""> 
		<jTemplate>
		<![CDATA[
			{
			#if($hy_maxny&&$hy_maxny.rows.size()>0)
				"latestValidMonth":"$!{hy_maxny.rows[0].ny}",
				"format":"Ym" 
			#end	
			}
		]]>
		</jTemplate>
		<vDs name="hy_maxny" refDtSrc="qcqy_maxny" fields="ny">
            <orderBy name = "ny" dir="desc"/>
		</vDs>
	</joutput>
	<joutput id="qcqy_year" name="迁出企业-有数据的年" description=""> 
		<jTemplate>
		<![CDATA[
			{
			#if($hy_year&&$hy_year.rows.size()>0)
				"options":[
					#foreach($row in $hy_year.rows)
	   						{"bm":"$!{row.nian}"}#if($velocityCount != $hy_year.rows.size()),#end
	   					#end
				],
				"defaultOp":"$!{hy_year.rows[0].nian}"
			#end	
			}
		]]>
		</jTemplate>
		<vDs name="hy_year" refDtSrc="qcqy_year" fields="nian">
			<orderBy name = "nian" dir="desc"/>
		</vDs>
	</joutput>
	<joutput id="qcqy_month" name="迁出企业-有数据的月份" description=""> 
		<jTemplate>
		<![CDATA[
			{
			#if($hy_month&&$hy_month.rows.size()>0)
				"options":[
					#foreach($row in $hy_month.rows)
	   						{"bm":"$!{row.yue}","name":"$!{row.yue}"}#if($velocityCount != $hy_month.rows.size()),#end
	   					#end
				],
				"defaultOp":"$!{hy_month.rows[0].yue}"
			#end
			}
		]]>
		</jTemplate>
		<vDs name="hy_month" refDtSrc="qcqy_month" fields="nian,yue">
			<filter name = "nian" refParam="year" value="2018"/>
			<orderBy name = "yue" dir="desc"/>
		</vDs>
	</joutput>
	<joutput id="qcqy_enlist" name="迁出企业列表"  description=""> 
		<jTemplate>
		<![CDATA[
			{
				#if($qcEns&&$qcEns.rows.size()>0)
				"qcqy":[
					#foreach($row in $qcEns.rows)
	   					{"swdjzh":"$!{row.swdjzh}","mc":"$!{row.mc}","czfp":"$!{row.czfp}","bnlj":"$!{row.bnlj}",
	   					"sntq":"$!{row.sntq}","dest":"$!{row.qcd}","escdate":"$!{row.qcrq}"}#if($velocityCount != $qcEns.rows.size()),#end
	   				#end
				],
				"total": $!{qcEns.total}
				#else
				"qcqy":[],"total":0
				#end
			}
		]]>
		</jTemplate>
		<vDs name="qcEns" refDtSrc="qcqy_details" paging="true" fields="swdjzh,mc,czfp,bnlj,sntq,qcd,qcrq">
			<filter name = "ny" value="201806"/>
			<orderBy name = "qcrq" dir="desc"/>
		</vDs>
	</joutput>
</pages>

