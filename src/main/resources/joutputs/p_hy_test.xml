<?xml version="1.0" encoding="UTF-8"?>
<pages>
	<joutput id="hy_df_ny" name="最大日期" description=""> 
		<jTemplate>
		<![CDATA[
			{
				"latestValidMonth":"201806",
				"format":"Ym" 
			}
		]]>
		</jTemplate>
	</joutput>
	<joutput id="hy_year" name="有数据的年" description=""> 
		<jTemplate>
		<![CDATA[
			{
				"options":[{"bm":"2018"}],
				"defaultOp":"2018"
			}
		]]>
		</jTemplate>
	</joutput>
	<joutput id="hy_month" name="有数据的月份选项" description=""> 
		<jTemplate>
		<![CDATA[
			{
				"options":[{"bm":"06","name":"06"},{"bm":"05"}],
				"defaultOp":"06"
			}
		]]>
		</jTemplate>
	</joutput>
	<joutput id="hy_all" name="行业分布"  description=""> 
		<jTemplate>
		<![CDATA[
			{
				"hyInfo":[
				#if($hy_tax_fb&&$hy_tax_fb.rows.size()>0)
					#foreach($row in $hy_tax_fb.rows)
	   					{"hybm":"$!{row.hybm}","hy":"$!{row.hy}","bnlj":"$!{row.bnlj_o}","sntq":"$!{row.sntq_o}",
	   					"srtb":"$!{row.tbzz}","sszb":"$!{row.srzb}","class":"hy_$!{row.hybm}","color":"$!{row.hycolor}"}
	   					#if($velocityCount != $hy_tax_fb.rows.size()),#end
	   				#end
				#end
				]
			}
		]]>
		</jTemplate>
		<vDs name="hy_tax_fb" refDtSrc="hy_tax_fb" fields="hybm,hy,bnlj_o,sntq_o,srzb,tbzz,hycolor">
			<filter name = "ny" value="201806"/>
			<orderBy name = "hybm" dir="asc"/>
		</vDs>
	</joutput>
	<joutput id="hy_one" name="某行业的税收情况"  description=""> 
		<jTemplate>
		<![CDATA[
			{
			#if($hy_one&&$hy_one.rows.size()>0)
				"bnlj":"$!{hy_one.rows[0].bnlj_o}",
				"sntq":"$!{hy_one.rows[0].sntq_o}",
				"tbzz":"$!{hy_one.rows[0].tbzz}"
			#else
				"bnlj":"",  
				"sntq":"",  
				"tbzz":"" 
			#end
			}
		]]>
		</jTemplate>
		<vDs name="hy_one" refDtSrc="hy_tax_fb" fields="bnlj_o,sntq_o,tbzz">
			<filter name = "hybm" value="K"/>
			<filter name = "ny" value="201806"/>
		</vDs>
	</joutput>
	<joutput id="hy_one_details" name="某行业的税收前30"  description=""> 
		<jTemplate>
		<![CDATA[
			{
				#if($hyEns&&$hyEns.rows.size()>0)
				"ens_hy":[
					#foreach($row in $hyEns.rows)
	   					{"swdjzh":"$!{row.swdjzh}","mc":"$!{row.mc}"}
	   					#if($velocityCount != $hyEns.rows.size()),#end
	   				#end
				],
				"total": $!{hyEns.total}
				#else
				"ens_hy":[],"total":0
				#end
			}
		]]>
		</jTemplate>
		<vDs name="hyEns" refDtSrc="hy_one_details" paging="true" fields="swdjzh,mc,bnlj_o,sntq_o,srzb">
			<filter name = "hybm" value="K"/>
			<filter name = "ny" value="201806"/>
			<orderBy name = "bnlj" dataType="double" dir="desc"/>
		</vDs>
	</joutput>
	<joutput id="hy_one_en" name="某企业的税收情况"  description="某行业中排名前30某企业的税收情况"> 
		<jTemplate>
		<![CDATA[
			{
			#if($hy_one_en&&$hy_one_en.rows.size()>0)
				"swdjzh":"$!{hy_one_en.rows[0].swdjzh}",
				"mc":"$!{hy_one_en.rows[0].mc}",
				"czfp":"$!{hy_one_en.rows[0].czfp_r}",
				"bnlj":"$!{hy_one_en.rows[0].bnlj_o}",
				"sntq":"$!{hy_one_en.rows[0].sntq_o}"
			#else
				"swdjzh":"",
				"mc":"",
				"czfp":"",
				"bnlj":"",  
				"sntq":""
			#end
			}
		]]>
		</jTemplate>
		<vDs name="hy_one_en" refDtSrc="hy_one_details" fields="swdjzh,mc,bnlj_o,sntq_o,hyzb,czfp_r">
			<filter name = "ny" value="201806"/>
			<filter name = "swdjzh" value="91330604146165334B"/>
		</vDs>
	</joutput>
</pages>

