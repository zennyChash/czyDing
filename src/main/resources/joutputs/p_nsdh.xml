<?xml version="1.0" encoding="UTF-8"?>
<pages>
	<joutput id="nsdh_df_ny" name="最大日期" description=""> 
		<jTemplate>
		<![CDATA[
			{
			#if($nsdh_maxny&&$nsdh_maxny.rows.size()>0)
				"latestValidMonth":"$!{nsdh_maxny.rows[0].ny}",
				"format":"Ym" 
			#end
			}
		]]>
		</jTemplate>
		<vDs name="nsdh_maxny" refDtSrc="nsdh_maxny" fields="ny"/>
	</joutput>
	<joutput id="nsdh_year" name="有数据的年" description=""> 
		<jTemplate>
		<![CDATA[
			{
			#if($nsdh_year&&$nsdh_year.rows.size()>0)
				"options":[
					#foreach($row in $nsdh_year.rows)
	   						{"bm":"$!{row.nian}"}#if($velocityCount != $nsdh_year.rows.size()),#end
	   					#end
				],
				"defaultOp":"$!{nsdh_year.rows[0].nian}"
			#end	
			}
		]]>
		</jTemplate>
		<vDs name="nsdh_year" fields="nian">
			<orderBy name = "nian" dir="desc"/>
		</vDs>
	</joutput>
	<joutput id="nsdh_month" name="有数据的月份选项" description=""> 
		<jTemplate>
		<![CDATA[
			{
			#if($nsdh_month&&$nsdh_month.rows.size()>0)
				"options":[
					#foreach($row in $nsdh_month.rows)
	   					{"bm":"$!{row.yue}"}#if($velocityCount != $nsdh_month.rows.size()),#end
	   				#end
				],
				"defaultOp":"$!{nsdh_month.rows[0].yue}"
			#end
			}
		]]>
		</jTemplate>
		<vDs name="nsdh_month" fields="ny,yue">
			<filter name = "nian" refParam="year" value="2018"/>
			<orderBy name = "ny" dir="desc"/>
		</vDs>
	</joutput>
	<joutput id="nsdh_basic" name="纳税大户"  description=""> 
		<jTemplate>
		<![CDATA[
			{
				#if($nsdh_basic&&$nsdh_basic.rows.size()>0)
					"total":"$!{nsdh_basic.rows[0].total}",
					"sszb_t":"$!{nsdh_basic.rows[0].sszb_t}",
					"bnlj":"$!{nsdh_basic.rows[0].bnlj}",
					"sntq":"$!{nsdh_basic.rows[0].sntq}",
					"tbzz":"$!{nsdh_basic.rows[0].tbzz}"
				#else
					"total":"",
					"sszb_t":"",
					"bnlj":"",
					"sntq":"",
					"tbzz":""
				#end
			}
		]]>
		</jTemplate>
		<vDs name="nsdh_basic" refDtSrc="nsdh_basic" fields="ny,tlimit,total,bnlj,sntq,sszb_t,tbzz">
			<filter name = "ny" value="201803"/>
			<filter name = "tlimit" refParam="taxLimit" value="0"/>
		</vDs>
	</joutput>
	<joutput id="nsdh_details" name="税收情况"  description="" > 
		<jTemplate>
		<![CDATA[
			{
				#if($nsdh_details&&$nsdh_details.rows.size()>0)
				"nsdh":[
					#foreach($row in $nsdh_details.rows)
	   					{"swdjzh":"$!{row.swdjzh}","mc":"$!{row.mc}","bnlj":"$!{row.bnlj}","sntq":"$!{row.sntq}","tbzz":"$!{row.tbzz}","srzb":"$!{row.sszb_t}"}
	   					#if($velocityCount != $nsdh_details.rows.size()),#end
	   				#end
				],
				"total": $!{nsdh_details.total}
				#else
				"nsdh":[],"total":0
				#end
			}
		]]>
		</jTemplate>
		<vDs name="nsdh_details" paging="true" fields="swdjzh,mc,bnlj,sntq,tbzz,sszb_t">
			<filter name = "ny" value="201803"/>
			<filter name = "tlimit" refParam="taxLimit" value="0"/>
			<orderBy name = "bnlj" dataType="double" dir="desc"/>
		</vDs>
	</joutput>
</pages>

